# Gitea Docker Environment Variables

# Gitea Version
GITEA_VERSION=latest

# Container Name
CONTAINER_NAME=gitea

# User and Group IDs
USER_UID=1000
USER_GID=1000

# Gitea Configuration
GITEA_ROOT_URL=http://localhost:3000/
GITEA_HTTP_PORT=3000
GITEA_SSH_PORT=22

