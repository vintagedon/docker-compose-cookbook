# Redis Docker Environment Variables

# Redis Version
REDIS_VERSION=latest

# Container Name
CONTAINER_NAME=redis

# Port
REDIS_PORT=6379

# Authentication
REDIS_PASSWORD=changeme

# Persistence
REDIS_APPENDONLY=yes

# Memory Management
REDIS_MAXMEMORY=256mb
REDIS_MAXMEMORY_POLICY=allkeys-lru

