# Ansible AWX Docker Environment Variables

# AWX Version
AWX_VERSION=latest

# Container Name Prefix
CONTAINER_NAME_PREFIX=awx

# Web Port
AWX_WEB_PORT=8080

# PostgreSQL Settings
POSTGRES_USER=awx
POSTGRES_PASSWORD=awxpass
POSTGRES_DB=awx

# AWX Security Settings
SECRET_KEY=awxsecret
AWX_ADMIN_USER=admin
AWX_ADMIN_PASSWORD=password

# Add any additional environment variables below